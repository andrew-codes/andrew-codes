{
  "root": "apps/andrew.codes",
  "sourceRoot": "apps/andrew.codes/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build"],
      "options": {
        "command": "yarn remix build"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "yarn rimraf dist"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -t ghcr.io/andrew-codes/andrew.codes:latest -f apps/andrew.codes/Dockerfile .",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": [],
  "implicitDependencies": []
}
