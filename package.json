    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "highlight.js": "^11.10.0",
    "isbot": "^5.1.17",
    "litefs-js": "^1.1.2",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.0.1",
    "mdx-bundler": "^10.0.3",
    "morgan": "^1.10.0",
    "on-finished": "^2.4.1",
    "path-to-regexp": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-is": "^18.3.1",
    "reading-time": "^1.5.0",
