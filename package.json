{
  "name": "@andrew-codes/andrew-codes",
  "description": "Website, profile and resume and James Andrew Smith.",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": "18.15.0",
    "yarn": ">=3.5.0"
  },
  "scripts": {
    "start": "yarn node index.js",
    "start/dev": "pm2-dev ./config/pm2.config.js",
    "start/dev/remix": "remix watch",
    "clean/build": "rimraf  ./build ./public/build ./server-build",
    "clean/dist": "rimraf ./dist",
    "clean": "rimraf ./node_modules/.cache && concurrently 'yarn run clean/build' 'yarn run clean/dist'",
    "prebuild": "yarn run clean",
    "build": "yarn run prebuild && concurrently 'yarn run build/server' 'yarn run build/remix' && yarn run postbuild",
    "postbuild": "yarn node --require esbuild-register scripts/package.ts",
    "build/remix": "remix-esbuild-override && remix build",
    "build/server": "yarn node --require esbuild-register scripts/build-server.ts"
  },
  "dependencies": {
